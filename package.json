{
  "name": "agda-mode",
  "main": "./lib/agda-mode",
  "version": "0.4.2",
  "description": "agda-mode for Atom",
  "activationCommands": {
    "atom-text-editor[data-grammar='source agda']": [
      "agda-mode:load",
      "agda-mode:input-symbol"
    ],
    "atom-text-editor[data-grammar='source lagda']": [
      "agda-mode:load",
      "agda-mode:input-symbol"
    ]
  },
  "repository": "https://github.com/banacorn/agda-mode",
  "license": "MIT",
  "engines": {
    "atom": ">0.180.0"
  },
  "dependencies": {
    "bluebird": "^3.0",
    "lisp-to-array": "^0.2",
    "lodash": "^3.10",
    "vue": "git+https://git@github.com/vuejs/vue.git#csp"
  }
}
